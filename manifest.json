{
    "name": "TCGA Toolbox",
    "description": "A collection of tools to process and analyze files from TCGA's open-access HTTP directory directly in the browser.",
    "version": "0.7.2",
    "app": {
        "launch": {
            "local_path": "main.html"
        }
    },
    "permissions": [
        "https://tcga-data.nci.nih.gov/tcgafiles/ftp_auth/distro_ftpusers/anonymous/tumor/*",
        "http://agalpha.mathbiol.org:10035/repositories/tcga?query=*",
        "unlimitedStorage"
    ],
    "icons": {
        "128": "icon_128.png",
        "16": "icon_16.png"
    }
}
