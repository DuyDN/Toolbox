{
    "manifest_version": 2,
    "name": "TCGA Toolbox",
    "description": "A scripting environment to retrieve, store, and analyze data from The Cancer Genome Atlas (TCGA) in the browser.",
    "version": "0.17.0",
    "app": {
        "launch": {
            "local_path": "main.html"
        }
    },
    "permissions": [
        "*://tcga-data.nci.nih.gov/",
        "unlimitedStorage"
    ],
    "optional_permissions": [
        "http://beta.sparql.uniprot.org/"
    ],
    "content_security_policy": "script-src 'self' https:// 'unsafe-eval'; object-src 'self'",
    "web_accessible_resources": [
        "main.html",
        "img/installed.png"
    ],
    "icons": {
        "128": "icon_128.png",
        "16": "icon_16.png"
    }
}
