{
    "name": "TCGA Toolbox",
    "description": "A collection of tools to process and analyze files from TCGA's open-access HTTP directory directly in the browser.",
    "version": "0.7.0",
    "app": {
        "launch": {
            "local_path": "main.html"
        }
    },
    "permissions": [
        "<all_urls>",
        "unlimitedStorage"
    ],
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
    "icons": {
        "128": "icon_128.png",
        "16": "icon_16.png"
    }
}
